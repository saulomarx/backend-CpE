{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "npm install && nodemon ./bin/www",
    "sequelize": "./node_modules/.bin/sequelize",
    "serve": "pm2 start ./bin/www --no-daemon",
    "db:migrate": "sequelize db:migrate",
    "db:seed": "sequelize db:migrate && sequelize db:seed:all",
    "db:seed:undo": "sequelize db:seed:undo"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "express": "~4.16.0",
    "morgan": "~1.9.0",
    "mysql2": "^1.5.3",
    "nodemon": "^1.17.4",
    "sequelize": "^4.37.7",
    "sequelize-cli": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.1.2",
    "mocha": "^5.1.1",
    "supertest": "^3.1.0"
  }
}
